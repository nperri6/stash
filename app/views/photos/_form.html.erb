<%= form_for @photo, url: new_photo_path, html: { multipart: true} do |f| %>

  <p><%= f.file_field :image %></p>
  <p><%= f.text_field :restaurant, value: nil, :placeholder => "Restaurant" %></p>
  <p><%= f.text_field :foodtags, value: nil, :placeholder => "Foodtags" %></p>
  <p><%= f.select  :pricetag, value: nil, [[]] %></p>
  <p><%= f.text_field :dish_name, value: nil, :placeholder => "Dish Name" %></p>
  <p><%= submit_tag 'Add Photo' %></br>

<% end %>
