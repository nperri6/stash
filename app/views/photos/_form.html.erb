<%= form_for photo, html: {multipart: true } do |f| %>

  <div class="top_form">
    <div class="top_form_left">
      <p><div class="photo_field_input"><%= f.text_field :dish_name, value: nil, id: "dish_name", :placeholder => "Dish Name" %></div></p>

      <p><div class="photo_field_input"><%= f.text_field :foodtags, id: "photo_foodtags", value: nil, :placeholder => "Foodtags" %></div></p>

      <p><div class="photo_field_label">Price Range:</div><div class="photo_field_input"><%= f.select :pricetag, [['$0..$10', "$" ], ['$10..$15', "$$" ], ['$15..$20', "$$$" ],['$20+', "$$$$" ]] %></div></p>
      <p><div class="photo_upload"><%= f.file_field :image, id: "upload_text_field", accept: "image/*" %></div></p>
    </div>

  <div class="top_form_right">
    <div id="photo_preview" /> </div>
  </div>
  <div class="clear_fix">
  </div>
  </div>

  <div class="map_container">
      <div id="map"></div>
  </div><br>

  <div class="bottom_form">
    <%= f.hidden_field :restaurant %>
    <p><input id="searchTextField" id="pac-input" class="controls" type="text" placeholder="search for restaurant here."></p>

    <%= f.hidden_field :lat, value: "", id:"lat_field"  %>
    <%= f.hidden_field :lng, value: "", id:"lng_field"  %>
    <p><%= submit_tag 'Add Photo', id:"add_photo_button" %></br>
  </div>
<% end %>
